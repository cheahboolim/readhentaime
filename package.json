{
	"name": "sveltekit-supabase-starter",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"generate-types": "supabase gen types typescript --local > src/lib/generated/supabase/types.ts"
	},
	"devDependencies": {
		"@eslint/js": "^9.8.0",
		"@playwright/test": "^1.45.3",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/adapter-cloudflare": "^7.0.4",
		"@sveltejs/kit": "^2.5.27",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@tailwindcss/typography": "^0.5.10",
		"@typescript-eslint/parser": "^8.0.0-alpha.54",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.8.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-svelte": "^2.45.1",
		"postcss": "^8.5.6",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.0",
		"tslib": "^2.6.3",
		"typescript": "^5.5.4",
		"typescript-eslint": "^8.0.0-alpha.54",
		"vite": "^5.4.4",
		"vite-plugin-pwa": "^1.0.0",
		"vitest": "^2.0.4"
	},
	"type": "module",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.850.0",
		"@supabase/auth-ui-svelte": "^0.2.9",
		"@supabase/ssr": "^0.5.1",
		"@supabase/supabase-js": "^2.44.4",
		"bcrypt": "^6.0.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"jsonwebtoken": "^9.0.2",
		"lucide-svelte": "^0.518.0",
		"tailwind-merge": "^3.3.1",
		"tailwindcss-animate": "^1.0.7"
	}
}
